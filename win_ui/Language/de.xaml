    <sys:Boolean x:Key="MachineTranslate">True</sys:Boolean>
    <sys:String x:Key="MachineTranslateWarning">Es scheint, dass die aktuell verwendete Sprache möglicherweise maschinell übersetzt wurde, was zu Fehlern in der Satzstruktur oder Wortwahl führen könnte. Bitte unterstützen Sie uns bei der Verbesserung der Übersetzung. Mit freundlichen Grüßen.</sys:String>

    <!--MainWindow Texts-->
    <sys:String x:Key="MainWindowTitle">ServerEase Startprogramm</sys:String>

    <!--SettingWindow Texts-->
    <sys:String x:Key="SettingWindowTitle">Optionen</sys:String>
    <sys:String x:Key="SaveButtonContent">Speichern</sys:String>
    <sys:String x:Key="CancelButtonContent">Stornieren</sys:String>
    <sys:String x:Key="GeneralSettingButtonContent">Allgemein</sys:String>

    <!--GeneralPage Texts-->
    <sys:String x:Key="LanguageLabelContent">Sprache</sys:String>
</ResourceDictionary>
